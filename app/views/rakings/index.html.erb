<h1>Výpis zkoušení</h1>

<table>
  <thead>
    <tr>
      <th>Konec</th>
      <th>Předmět</th>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rakings.each do |raking| %>
      <tr>
      	<td><%= raking.name %></td>
      	<td><%= raking.description %></td>
      	<td><%= raking.subject.name %></td>
      	<td><%= raking.additional_info_url %></td>
        <td><%= raking.end %></td>
        <td><%=  %></td>

        <td><%= raking.description %></td>
        <td><%= link_to 'Show', raking %></td>
        <td><%= link_to 'Edit', edit_raking_path(raking) %></td>
        <td><%= link_to 'Destroy', raking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

