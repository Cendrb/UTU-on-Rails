<h1>Listing additional_infos</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @additional_infos.each do |additional_info| %>
      <tr>
        <td><%= additional_info.name %></td>
        <td><%= additional_info.url %></td>
        <td><%= link_to 'Show', additional_info %></td>
        <td><%= link_to 'Edit', edit_additional_info_path(additional_info) %></td>
        <td><%= link_to 'Destroy', additional_info, method: :delete, data: { confirm: 'Jste si jisti?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Additional info', new_additional_info_path %>
